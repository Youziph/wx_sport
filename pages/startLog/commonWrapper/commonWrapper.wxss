/* pages/startLog/commonWrapper/commonWrapper.wxss */
.startBody{
  width: 100vw;
  height: 100vh;
  background-color: #ccc;
  position: relative
}

.startBody .start{
  box-shadow: 0px 0px 10px .8px #f2f2f2;
  border-radius: 5px;
  position: absolute;
  top: 50%;
  left: 50%;
  width: 80%;
  height: 60vh;
  transform: translate(-50%, -50%);
  background-color: white;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}